{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["eslint:recommended", "plugin:prettier/recommended"],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"prettier/prettier": [
			{
				"trailingComma": "all",
				"tabWidth": 4,
				"semi": false,
				"singleQuote": true,
				"bracketSpacing": true,
				"eslintIntegration": true,
				"printWidth": 120
			}
		]
	}
}
